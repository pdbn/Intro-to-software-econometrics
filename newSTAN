data {
  int<lower=1> n;  
  int<lower=1>k;   
  int y[n];       
  matrix[n,k] x;  
}


parameters {
  real intercept;
  vector[k] coeff;  
}


model {
  vector[n] p;
  p= inv_logit(intercept + x*coeff[1:k]);  
  y~bernoulli(p);
  intercept~normal(1, 0.5);  
  coeff[1] ~normal(0,1);
  coeff[2] ~ normal(0,1);
  coeff[3] ~normal (2, 0.7);
  for(i in 4:k)
    coeff[i] ~lognormal(0,1);
}
